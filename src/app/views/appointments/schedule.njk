 {% for appointment in appointments | sort(attribute='date') %}
  <li class="provider">
    <div>
      <img src="/files/{{ appointment.user.avatar }}" />
      <strong>{{ appointment.user.name }} <br>
      <span>{{ appointment.user.email}}</span></strong>
    </div>
    <span>{{ appointment.date | date("HH:mm") }}</span>
  </li>
 {% endfor %}
